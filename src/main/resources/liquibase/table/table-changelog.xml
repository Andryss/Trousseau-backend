<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <preConditions>
        <dbms type="postgresql"/>
    </preConditions>

    <include file="categories.sql" relativeToChangelogFile="true"/>
    <include file="items.sql" relativeToChangelogFile="true"/>
    <include file="items_history.sql" relativeToChangelogFile="true"/>
    <include file="bookings.sql" relativeToChangelogFile="true"/>
    <include file="favourites.sql" relativeToChangelogFile="true"/>
    <include file="subscriptions.sql" relativeToChangelogFile="true"/>
    <include file="notifications.sql" relativeToChangelogFile="true"/>
    <include file="key_storage.sql" relativeToChangelogFile="true"/>

</databaseChangeLog>